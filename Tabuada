#include <stdio.h>

int main(){
    
    int nmr;
    int tot;
    printf("Digite um número para ver a sua tabuada: ");
    scanf("%d", &nmr);
    printf("Agora até onde você quer ir com a tabuada: ");
    scanf("%d", &tot);
    tot++;
    
    for (int i = 1; i < tot; i++)
        printf("\n%d x %d = %d", nmr, i, nmr * i);
    return 0;
}
